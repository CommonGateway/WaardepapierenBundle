<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <defaults autowire="true" autoconfigure="true" />
        <service public="true" id="CommonGateway\WaardepapierenBundle\Service\WaardepapierenService" >
            <argument key="$entityManager" type="service" id="Doctrine\ORM\EntityManagerInterface"/>
            <argument key="$objectEntityService" type="service" id="App\Service\ObjectEntityService"/>
            <argument key="$twig" type="service" id="Twig\Environment"/>
            <argument key="$qrCode" type="service" id="Endroid\QrCode\Factory\QrCodeFactoryInterface"/>
        </service>
        <service public="true" id="CommonGateway\WaardepapierenBundle\ActionHandler\WaardepapierenHandler" />
    </services>
</container>
